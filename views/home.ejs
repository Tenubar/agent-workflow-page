                    </div>

                    <div class ="agents-wrapper">
                        <!--Active Proyects--> 
                        <div class ="agents-dashboard"> 

                            <div id ="button-new-agent">
                                <div>Create New</div>
                                <span class="material-symbols-outlined">add</span>
                            </div>

                        </div>
                    </div>

                    <div class = "border-agents">
                        <h5>Workflows</h5>
                    </div>

                    <div class ="workflows-wrapper">

                <div class ="workflows-dashboard">


                    <div id ="button-new-workflow">
                        <div>Create New</div>
                        <span class="material-symbols-outlined">add</span>
                    </div>

                    <div class ="suggested-workflow-1">
                        <span class="material-symbols-outlined">lan</span>
                        <h2>Product Name Generator</h2>
                        <small class = "text-muted"></small>
                        <div class ="run-agent">
                            <button>Run</button>
                            <span class="material-symbols-outlined">prompt_suggestion</span>
                        </div>
                    </div>

                </div>
                </div>
                </div>
