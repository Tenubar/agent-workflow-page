                </div> -->

                <div class ="insights">
                    <!--Active Proyects--> 
                    <div class ="proyects">
                        <div class ="middle">
                            <div class = "left">
                                <h5>Time Saved</h5>
                                <span class="material-symbols-outlined">show_chart</span>
                            </div>
                            <h1>0</h1>
                        </div>
                        <small class="text-muted">Last 24 Hours </small>
                    </div>
                    <!--Team Members--> 
                    <div class ="team">
                        <div class ="middle">
                            <div class = "left">
                                <h5>Agents</h5>
                                <span class="material-symbols-outlined">group</span>
                            </div>
                            <h1><%= agentCount %></h1>
                        </div>
                        <small class="text-muted">Last 24 Hours </small>
                    </div>
                    <!--Content Pieces--> 
                    <div class ="content">
                        <div class ="middle">
                            <div class = "left">
                                <h5>Workflows</h5>
                                <span class="material-symbols-outlined">description</span>
                            </div>
                            <h1>0</h1>
                        </div>
                        <small class="text-muted">Last 24 Hours </small>
                    </div>
                    <!--Ai Conversations--> 
                    <div class ="conversations">
                        <div class ="middle">
                            <div class = "left">
                                <h5>Ai Conversations</h5>
                                <span class="material-symbols-outlined">chat</span>
                            </div>
                            <h1>0</h1>
                        </div>
                        <small class="text-muted">Last 24 Hours </small>
                    </div>
                </div>

                <div>
                    <div class = "border-agents">
